{
  "version": "1.32",
  "description": "GNU Tar provides the ability to create tar archives, as well as various other kinds of manipulation.",
  "platforms": [
    "linux/amd64",
    "linux/arm64/v8",
    "linux/arm/v7",
    "linux/arm/v6",
    "linux/ppc64le",
    "linux/s390x",
    "linux/riscv64"
  ],
  "imageName": "gnu-tar",
  "tagSuffix": "-slim",
  "tagFormats": [
    "type=pep440,pattern={{raw}}",
    "type=pep440,pattern={{major}}.{{minor}}",
    "type=pep440,pattern={{major}}"
  ]
}