  "platforms": [
    "linux/amd64",
    "linux/arm64/v8",
    "linux/arm/v7",
    "linux/arm/v6",
    "linux/ppc64le",
    "linux/s390x",
    "linux/riscv64"
  ],
  "qemu": true,
  "imageName": "dash",
  "tagFormats": [
    "type=pep440,pattern={{version}}",
    "type=pep440,pattern={{major}}",
    "type=pep440,pattern={{major}}.{{minor}}",
    "type=pep440,pattern={{major}}.{{minor}}.{{patch}}"
  ]
}